{
	"name": "3d2p.vscode",
	"publisher": "3d2p",
	"displayName": "3D2P - 3D Print Projects",
	"author": "Gerrit 'Geaz' Gazic",
	"license": "MIT",
	"description": "3D Print Projects Management in VS Code",
	"version": "0.5.2",
	"icon": "logo.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/geaz/3D2P-3DPrintProjects.git"
	},
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:3d2p.cmd.initProject",
		"workspaceContains:3D2P.json"
	],
	"contributes": {
		"commands": [
			{
				"command": "3d2p.cmd.initProject",
				"title": "3D2P: Initialize 3D Print Project"
			},
			{
				"command": "3d2p.cmd.setProjectData",
				"title": "3D2P: Set Project Data"
			},
			{
				"command": "3d2p.cmd.addStl",
				"title": "3D2P: Add STL"
			},
			{
				"command": "3d2p.cmd.packProject",
				"title": "3D2P: Create 3MF"
			},
			{
				"command": "3d2p.cmd.open3mf",
				"title": "3D2P: Open 3MF"
			},
			{
				"command": "3d2p.cmd.openStl",
				"title": "3D2P: Open STL"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "3d2p.cmd.open3mf",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "3d2p.cmd.open3mf",
					"when": "resourceExtname == .3mf"
				},
				{
					"command": "3d2p.cmd.openStl",
					"when": "resourceExtname == .stl"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npx npm-run-all compile:*",
		"compile:apps": "npx webpack --config webpack.config.js",
		"compile:vsc": "npx tsc -p ./",
		"watch": "npx tsc -watch -p ./"
	},
	"devDependencies": {
		"3d2p.react.components": "file:../app",
		"@types/command-exists": "1.2.0",
		"@types/glob": "7.1.1",
		"@types/node": "12.12.32",
		"@types/react": "16.9.34",
		"@types/react-dom": "16.9.7",
		"@types/styled-components": "5.1.0",
		"@types/vscode": "1.43.0",
		"@types/webgl2": "0.0.5",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"command-exists": "1.2.9",
		"glob": "7.1.6",
		"npm-run-all": "4.1.5",
		"ts-loader": "6.2.2",
		"typescript": "3.8.3",
		"webpack": "4.42.1",
		"webpack-cli": "3.3.11"
	}
}
