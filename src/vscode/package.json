{
	"name": "3d2p.vscode",
	"publisher": "3d2p",
	"displayName": "3D2P - 3D Print Projects",
	"author": "Gerrit 'Geaz' Gazic",
	"license": "MIT",
	"description": "3D Print Projects Management in VS Code",
	"version": "0.5.2",
	"icon": "logo.png",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/geaz/3D2P-3DPrintProject.git"
	},
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"main": "./out/extension.js",
	"activationEvents": [
		"onCommand:3d2p.cmd.initProject",
		"workspaceContains:3D2P.json"
	],
	"contributes": {
		"commands": [
			{
				"command": "3d2p.cmd.initProject",
				"title": "3D2P: Initialize 3D Print Project"
			},
			{
				"command": "3d2p.cmd.setProjectData",
				"title": "3D2P: Set Project Data"
			},
			{
				"command": "3d2p.cmd.addStl",
				"title": "3D2P: Add STL"
			},
			{
				"command": "3d2p.cmd.setStlInfo",
				"title": "3D2P: Set STL Info"
			},
			{
				"command": "3d2p.cmd.packProject",
				"title": "3D2P: Create 3MF"
			},
			{
				"command": "3d2p.cmd.open3mf",
				"title": "3D2P: Open 3MF"
			},
			{
				"command": "3d2p.cmd.openStl",
				"title": "3D2P: Open STL"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "3d2p.cmd.open3mf",
					"when": "false"
				},
				{
					"command": "3d2p.cmd.openStl",
					"when": "false"
				},
				{
					"command": "3d2p.cmd.setStlInfo",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "3d2p.cmd.open3mf",
					"when": "resourceExtname == .3mf"
				},
				{
					"command": "3d2p.cmd.openStl",
					"when": "resourceExtname == .stl"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npx tsc -p ./",
		"watch": "npx tsc -watch -p ./"
	},
	"devDependencies": {
		"3d2p.react.app": "0.5.0",
		"@types/command-exists": "1.2.0",
		"@types/glob": "7.1.1",
		"@types/node": "12.12.32",
		"@types/vscode": "1.43.0",
		"@types/webgl2": "0.0.5",
		"command-exists": "1.2.9",
		"glob": "7.1.6",
		"typescript": "3.8.3"
	}
}
